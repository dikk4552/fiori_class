<mvc:View controllerName="project1.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt">
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader>
                <Image height="30px"
					   class="sapUiSmallMarginBegin"
					   src="image/logo.svg" />
                <MenuButton enabled="true" text="메뉴" buttonMode="Split" width="auto">
                    <menu>
                        <Menu>
                            <MenuItem id="menu1" text="물품 요청 목록" press="onRequest"/>
                            <MenuItem text="물품 요청 현황" press="onRequestState"/>
                            <MenuItem text="협력업체 목록" press="onCompany"/>
                            <MenuItem text="부품 목록" press="onProductList"/>
                        </Menu>

                    </menu>
                </MenuButton>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <App class="myAppDemoWT" id="app">
            </App>
        </tnt:mainContents>
    </tnt:ToolPage>



</mvc:View>
